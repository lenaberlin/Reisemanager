        <name>Gebüren_zu_Gesamtkosten</name>
        <order/>
        <script><![CDATA[function onChange() {
   //Type appropriate comment here, and begin script below
	var geb = g_form.getDecimalValue("geschaetze_geb_hren");
	var fahr = g_form.getDecimalValue("geschaetzte_fahrtkosten");
	var unt = g_form.getDecimalValue("geschaetzte_unterkunftskosten");
	var gesamt = parseFloat(geb) + parseFloat(fahr) + parseFloat(unt);
	var v = "";
	var tmp = g_form.getValue("geschaetze_geb_hren");
	for(var i = 0; tmp[i] != ";"; i++){
		v += tmp[i];
	}
	//alert(v + ";" + gesamt);
	g_form.setValue("gesamtkosten", v + ";" + gesamt);
}]]></script>
